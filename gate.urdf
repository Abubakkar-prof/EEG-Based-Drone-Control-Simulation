<?xml version="1.0" ?>
<robot name="gate">
  <material name="orange">
    <color rgba="1 0.5 0 1"/>
  </material>

  <link name="base_link">
    <inertial>
      <mass value="0"/> <!-- Static -->
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <inertia ixx="0" ixy="0" ixz="0" iyy="0" iyz="0" izz="0"/>
    </inertial>
    
    <!-- Left Post -->
    <visual>
      <origin rpy="0 0 0" xyz="-1 0 1"/>
      <geometry>
        <cylinder length="2.0" radius="0.1"/>
      </geometry>
      <material name="orange"/>
    </visual>
    <collision>
      <origin rpy="0 0 0" xyz="-1 0 1"/>
      <geometry>
        <cylinder length="2.0" radius="0.1"/>
      </geometry>
    </collision>

    <!-- Right Post -->
    <visual>
      <origin rpy="0 0 0" xyz="1 0 1"/>
      <geometry>
        <cylinder length="2.0" radius="0.1"/>
      </geometry>
      <material name="orange"/>
    </visual>
    <collision>
      <origin rpy="0 0 0" xyz="1 0 1"/>
      <geometry>
        <cylinder length="2.0" radius="0.1"/>
      </geometry>
    </collision>

    <!-- Top Bar -->
    <visual>
      <origin rpy="0 1.5708 0" xyz="0 0 2"/>
      <geometry>
        <cylinder length="2.2" radius="0.1"/>
      </geometry>
      <material name="orange"/>
    </visual>
    <collision>
      <origin rpy="0 1.5708 0" xyz="0 0 2"/>
      <geometry>
        <cylinder length="2.2" radius="0.1"/>
      </geometry>
    </collision>
  </link>
</robot>
